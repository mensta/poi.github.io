if(isset($_POST["botan"]))
{
   $moji='"LVL:  ';
   $moji.=htmlspecialchars($_POST["data"],ENT_NOQUOTES);
   $moji.='"';

   $moji.=",";

   $moji.='"Nick: ';
   $moji.=htmlspecialchars($_POST["namae"],ENT_NOQUOTES);
   $moji.='"';

//   $moji.=",";


//   $moji.='"';
//    $moji.=date("Y/m/d G:H:s");
//   $moji.='"';

   $moji.="\r\n";

   $file=@fopen("log.txt","a");
   if(!$file)
   {
   }
   else
   {
      fputs($file,$moji);
      fclose($file);
   }

   $_POST["botan"]=NULL;
}
