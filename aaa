<script>
    var $form = $('#sparkstar'),
        $trigger = $('#sendMessageTrigger'),
        $iframe = $('iframe[name="sendMessage"]');
 
    $trigger.click(function() {
        $form.submit();
 
        $form[0].reset();   // フォームをリセット
 
        return false;
    });
 
    $form.submit(function() {
        $iframe.unbind().bind('load', function() {
            // 結果が見たい場合はココで処理
    location.href = "#";
        });
    });
</script> --->

